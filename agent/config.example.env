# Configuration for Claude Dash Agent
# Copy this file to .env and customize for your setup

# URL of the central Claude Dash server
# Replace <server-ip> with the IP address of the machine running the server
SERVER_URL=http://<server-ip>:3001

# How often to report sessions (in milliseconds)
REPORT_INTERVAL=5000

# Optional: Override machine name (defaults to hostname)
# MACHINE_NAME=my-laptop
